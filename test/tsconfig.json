{
  "extends": "../tsconfig.json",
  "compilerOptions": {
  "module": "ESNext",
  "moduleResolution": "bundler",
  "target": "ES2022",
    "baseUrl": "..",
    "paths": {
      "#nuxt/mongoose": ["./test/mocks/nuxt-mongoose"],
      "~~/server/models/*": ["./test/mocks/nuxt-mongoose"],
      "~/server/models/*": ["./test/mocks/nuxt-mongoose"]
    },
    "types": [
      "node",
      "vitest/globals"
    ]
  },
  "include": [
    "./**/*.ts"
  ]
}
